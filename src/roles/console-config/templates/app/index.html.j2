    <nav class="navbar navbar-dark" style="background-color: #3399ff">
        <img src = "/img/SIDN-logo.svg" width="50" height="50"/>
        <ul class="navbar-nav bd-navbar-nav flex-row">
          <li class="nav-item" id="top-title">
            <a class="navbar-brand" href="/">Labs Hadoop Provisioning</a>
          </li>
        </ul>
      <ul class="navbar-nav flex-row ml-md-auto d-none d-md-flex">
      <a href="/client-config/client-config.tar.gz" download>
        <button class="btn btn-outline-light" type="submit">Download Client configuration</button>
      </a>
    </ul>
    </nav>

    <div class="container-fluid d-flex flex-column min-vh-100">
      <div class="row flex-xl-nowrap">
        
  <div class="flex-shrink-0 p-3 bg-white sidebar vh-100" style="width: 280px;">


    <ul class="list-unstyled ps-0">
      <li class="mb-1">
        <button class="btn btn-toggle align-items-center rounded collapsed" data-bs-toggle="collapse" data-bs-target="#home-collapse" aria-expanded="true">
          Services
        </button>
        <div class="collapse show" id="home-collapse">
          <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
            <li>
             <a {% if service_item.name == 'hdfs' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/hdfs.html">
               HDFS
             </a></li><li>
             <a {% if service_item.name == 'yarn' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/yarn.html">
               YARN
             </a></li><li>
             <a {% if service_item.name == 'impala' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/impala.html">
               Impala
             </a></li><li>
             <a {% if service_item.name == 'spark' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/spark.html">
               Spark
             </a></li><li>
             <a {% if service_item.name == 'hive' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/hive.html">
               Hive
             </a></li><li>
            <a {% if service_item.name == 'hue' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/hue.html">
               Hue
             </a></li><li>
            <a {% if service_item.name == 'Superset' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/superset.html">
               Hue
             </a></li><li>
             <a {% if service_item.name == 'livy' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/livy.html">
               Livy
             </a></li><li>
             <a {% if service_item.name == 'zookeeper' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/zookeeper.html">
               Zookeeper
             </a></li><li>
            <a {% if service_item.name == 'solr' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/solr.html">
               SOLR
             </a></li><li>
             <a {% if service_item.name == 'ranger' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/ranger.html">
               Ranger
             </a></li><li>
             <a{% if service_item.name == 'monitor' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/monitor.html">
               Monitor
             </a></li><li>
             <a {% if service_item.name == 'gateway' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/service/gateway.html">
               Gateway
             </a></li>
          </ul>
        </div>
      </li>      
      <li class="mb-1">
        <button class="btn btn-toggle align-items-center rounded collapsed" data-bs-toggle="collapse" data-bs-target="#dashboard-collapse" aria-expanded="false">
          Additional
        </button>
        <div class="collapse show" id="dashboard-collapse">
          <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
            <li>
              <a {% if service_item.name == 'tools' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/tools.html">
                Tools
              </a>
            </li>
            <li>
              <a {% if service_item.name == 'hostmap' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/hostmap.html">
                Host Mapping
              </a>
            </li>
            <li>
              <a {% if service_item.name == 'version' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/version.html">
                Versions
              </a>
            </li>
          </ul>
        </div>
      </li>
      <li class="mb-1">
        <button class="btn btn-toggle align-items-center rounded collapsed" data-bs-toggle="collapse" data-bs-target="#dashboard-collapse" aria-expanded="false">
          Help
        </button>
        <div class="collapse show" id="dashboard-collapse">
          <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
            <li>
              <a {% if service_item.name == 'security' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/help/security.html">
                Security
              </a>
            </li>
            <li>
              <a {% if service_item.name == 'about' %}class="link-dark rounded btn-toggle-nav-selected" {% else %} class="link-dark rounded" {% endif %} href="/help/about.html">
                About
              </a>
            </li>
          </ul>
        </div>
      </li>
    </ul>
  </div>
      <div class="main-box col-12 col-md-9 col-xl-8 py-md-3 pl-md-5 flex-column" role="main">
        {% include './templates/app/' + service_item.path + '/' + service_item.src %}
      </div>
  </div>

</div>

    </div>
   <footer class="footer">
      <div class="container">
